    "lodash": "^4.17.21",
    "plotly.js": "^2.35.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.7",
    "react-dom": "^18.3.1",
    "react-mathquill": "^1.0.4",
    "react-plotly.js": "^2.6.0",
    "react-resizable": "^3.0.5",
    "react-scroll-wheel-handler": "^2.2.0",
    "react-sparklines": "^1.7.0",
    "save-svg-as-png": "^1.4.17",
