    "@types/react-plotly.js": "^2.6.3",
    "@types/react-resizable": "^3.0.8",
    "@types/react-sparklines": "^1.7.5",
    "@vitejs/plugin-react": "^4.7.0",
    "sass": "^1.83.1",
    "scrns": "^0.2.1",
